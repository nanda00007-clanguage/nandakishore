#include<stdio.h>
struct mark
{
    int sub1;
    int sub2;
    int sub3;
}m;
struct sist
{
    char name;
    int regno;
    struct mark m;
}
s[3];
int main()
{
   struct sist;
   for(int i=0;i<3;i++)
   {
       printf("enter the name%d:",i+1);
       scanf("%s",&s[i].name);
       printf("enter the regno%d:",i+1);
       scanf("%d",&s[i].regno);
       printf("enter the mark1:");
       scanf("%d",&s[i].m.sub1);      
       printf("enter the mark2:");
       scanf("%d",&s[i].m.sub2);
       printf("enter the mark3:");
       scanf("%d",&s[i].m.sub3);
       printf("total marks is%d",s[i].m.sub1+s[i].m.sub2+s[i].m.sub3);
       
       
       
       printf("\n\n");
   }   
 return 0;
 
   }
