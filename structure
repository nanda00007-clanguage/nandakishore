#include<stdio.h>
struct sist
{
    char name;
    int regno;
    float mark;
    
}
s[3];
int main()
{
   struct sist;
   for(int i=0;i<3;i++)
   {
       printf("enter the name%d:",i+1);
       scanf("%s",&s[i].name);
       printf("enter the regno%d:",i+1);
       scanf("%d",&s[i].regno);
       printf("enter the mark%d:",i+1);
       scanf("%f",&s[i].mark);
       printf("\n");
   }
   return 0;
}
