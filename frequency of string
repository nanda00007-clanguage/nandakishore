#include<stdio.h>
#include<string.h>
int main()
{
	char str[100],ch;
	int count =0;
	printf("enter a string:");
	fgets(str, sizeof(str),stdin);
	printf("enter a character:");
	scanf("%c",&ch);
	for(int i=0; str[i]!=0; i++)
	{
	    if(ch==str[i])
	    count++;
	}
	printf("Frequency of %c = %d",ch,count);
	return 0;
		
}
