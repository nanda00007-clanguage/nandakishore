#include<stdio.h>
#include<string.h>
int main()
{
int i,j, count=0;
char str1[4]="nan";
 for(i=0,j=(strlen(str1)-1);i<7,j>0;i++,j--)
         if (str1[i]!=str1[j])
         {
             printf("not palindrome");
            count=1;
            break;
         }
    if(count==0)
       printf("palindrome");
    return 0;
}
    


