#include<stdio.h>    
#include<string.h>
int main()
{
    char str1[10];
    char c='p';
    printf("enter name:");
    fgets(str1,sizeof(str1),stdin);
    puts(str1);
    for(int i=0;i<strlen(str1);i++)
    {
        if(str1[i]==c)
         printf("character found");
        else
         printf("not found");
         break;
    }
    
    return 0;
    
}
